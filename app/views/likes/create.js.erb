<% if @likable.class.name == "Post" %>
  $("#likes<%= @likable.id %>").html(" <%= j render(partial: 'posts/like', :locals => {:post => @likable}) %> ");
<% else %>
  $("#like<%= @likable.id %>").html(" <%= j render(partial: 'comments/comment_like', :locals => {:comment => @likable}) %> ");
<% end %>